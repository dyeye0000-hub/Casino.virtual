<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rufus.casinovirtual">
    <application
        android:label="@string/app_name"
        android:icon="@mipmap/ic_launcher"
        android:theme="@style/Theme.MaterialComponents.DayNight.NoActionBar">
        <activity android:name=".admin.AdminActivity" />
        <activity android:name=".games.SlotsActivity" />
        <activity android:name=".games.RouletteActivity" />
        <activity android:name=".auth.RegisterActivity" />
        <activity android:name=".auth.LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>
